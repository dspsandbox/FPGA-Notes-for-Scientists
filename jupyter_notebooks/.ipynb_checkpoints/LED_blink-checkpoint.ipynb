   "source": [
    "import pynq\n",
    "ol = pynq.Overlay(\"LED_blink.bit\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "blinkFreq = 1\n",
    "clkFreq = 50e6\n",
    "incr = int(2**32 * blinkFreq / clkFreq)\n",
    "\n",
    "#Enable counter\n",
    "ol.axi_gpio_0.channel1.write(val=1, mask=0x1)\n",
    "\n",
    "#Set counter increment\n",
    "ol.axi_gpio_0.channel2.write(val=incr, mask=0xffffffff)"
   ]
  }
